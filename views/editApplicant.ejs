<!DOCTYPE html>
<html lang="en">

<%- include('./components/header.ejs')%>

<body>

<%- include('./components/navbar.ejs')%>


  <div class="container">
      <form action="/pelamar/editFix/<%=data.id%>" method ="POST">
          <div class="form-group">
            <label for="name">Name</label>
            <input type="text" class="form-control" name="name" placeholder="Enter Name" value="<%=data.name%>">
          </div>
          <div class="form-group">
              <label for="exampleInputPassword1">Password</label>
              <input type="password" class="form-control" name="password" id="exampleInputPassword1" placeholder="Password" value="<%=data.password%>">
          </div>
          <div class="form-group">
            <label for="age">Age</label>
            <input type="number" class="form-control" name="age" placeholder="Enter Age" value="<%=data.age%>">
          </div>
          <div class="form-group">
              <label for="job">Job Wanted</label>
              <input type="text" class="form-control" name="job" placeholder="Enter Job" value="<%=data.job%>">
          </div>
          <div class="form-group">
              <label for="salary">Salary Wanted</label>
              <input type="text" class="form-control" name="salary" placeholder="Enter Salary" value="<%=data.salary%>">
          </div>
          <button type="submit" class="btn btn-primary">Edit</button>
        </form>
  </div>
</body>
</html>